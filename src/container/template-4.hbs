<header class="header">
    <div>
        {{>logo}}
    </div>
    <nav class="header_nav">
        <div class="header_nav_right">
            {{>nav list=header}}
        </div>
    </nav>
</header>

{{>mainTemp4}}

<main class="featured">
    {{#each featured}}
    {{>featuredBlog data=this}}
    {{/each}}
</main>


<div class="actions">
    {{#each actions}}
    <div class="actions_content">
        <h3 class="actions_title">{{heading}}</h3>



        <a href="{{url}}" class="actions_button">{{text}}</a>

    </div>
    {{/each}}
</div>



<footer class="footer">
    <div>
        {{>logo}}
        <span class="footer_copyright">Copyright 2023-2024</span>
    </div>
    {{#each footer}}
    <div>
        <span class="foter_title">{{title}}</span>
        {{>nav list=links isVertical=true}}
    </div>
    {{/each}}
</footer>